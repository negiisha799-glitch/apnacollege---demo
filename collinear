#include <stdio.h>
int main() {
    float x1, y1, x2, y2, x3, y3;

    printf("Enter x1 y1: ");
    scanf("%f %f", &x1, &y1);
    printf("Enter x2 y2: ");
    scanf("%f %f", &x2, &y2);
    printf("Enter x3 y3: ");
    scanf("%f %f", &x3, &y3);

    if ((y2-y1) * (x3-x1) == (y3-y1) * (x2-x1))
    printf("The points are collinear (lie on a straight line)\n");
    else
    printf("The points are not collinear\n");
    return 0;

}
