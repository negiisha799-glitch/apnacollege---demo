#include<stdio.h>
int main() {
    struct pokemon{ // user defined data type
        int hp;
        int speed;
        int attack;
        char tier; // S,A,G
    };
    struct pokemon pikachu;
   pikachu.attack=60;
    pikachu.hp=50;
    pikachu.speed=100;
    pikachu.tier='A';

    printf("%d\n",pikachu.attack);

    struct pokemon charizard;
    charizard.attack=130;
    charizard.hp=80;
    charizard.speed=80;
    charizard.tier='S';


struct pokemon mewtwo;
    mewtwo.attack=170;
    mewtwo.hp=150;
    mewtwo.speed=200;
    mewtwo.tier='G';

return 0;
}
