#include <stdio.h>
#include <limits.h>
int main() {
    int arr[7] = {-10,-4,-200,-80,-19,-5,-12};
    int max = INT_MIN;  
    int secondLargest = INT_MIN;
    for(int i=1;i<=6;i++){
        if(max,arr[i]){
            max = arr[i];
        }
    }
    for(int i=0;i<=6;i++){
        if(arr[i]!=max && secondLargest<arr[i]){
            secondLargest = arr[i];
        }
    }
    printf("%d\n", max);
    return 0;
}
