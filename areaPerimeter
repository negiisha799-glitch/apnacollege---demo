#include <stdio.h>
int main() {
    int l;
    printf("Enter length: ");
    scanf("%d", &l);
    int b;
    printf("Enter breadth: ");
    scanf("%d", &b);
    int a = l*b;
    int p = 2*(l+b);
    if (a>p) {
        printf("Area is greater than Perimeter");
    }
    else if (p>a) {
        printf("Perimeter is greater than Area");
    }
    else {
        printf("Area is equal to Perimeter");
    }

}
