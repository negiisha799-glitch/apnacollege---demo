#include <stdio.h>
int main() {
    int a, b, c;
    printf("Enter age of ram: ");
    scanf("%d", &a);
    printf("Enter age of shyam: ");
    scanf("%d", &b);
    printf("Enter age of ajay: ");
    scanf("%d", &c);
    if (a<b && a<c) {
    printf("Youngest age is of Ram: %d", a);
    }
    else if (b<a && b<c) {
        printf("Youngest age is of Shyam: %d", b);
    }
    else {
        printf("Youngest age is of Ajay: %d", c);
    }
    return 0;

}
